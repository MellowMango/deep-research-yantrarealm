{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/research",
      "dest": "src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
